{
    "name": "150657373-nikolajve/laravel-exception-analyzer",
    "description": "This is my package laravel-exception-analyzer",
    "keywords": [
        "NikolajVE",
        "laravel",
        "laravel-exception-analyzer"
    ],
    "homepage": "https://github.com/150657373-nikolajve/laravel-exception-analyzer",
    "license": "MIT",
    "authors": [
        {
            "name": "NikolajVE",
            "email": "10503092@ucn.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^11.0||^12.0",
        "prism-php/prism": "^0.99.0",
        "spatie/laravel-flare": "^2.5",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelExceptionAnalyzer\\": "src/",
            "LaravelExceptionAnalyzer\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelExceptionAnalyzer\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelExceptionAnalyzer\\LaravelExceptionAnalyzerServiceProvider"
            ],
            "aliases": {
                "LaravelExceptionAnalyzer": "LaravelExceptionAnalyzer\\Facades\\LaravelExceptionAnalyzer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
